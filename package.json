{
  "name": "searchpicture",
  "productName": "Search Picture",
  "version": "0.1.0",
  "private": true,
  "main": "./app/main.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "el": "cross-env NODE_ENV=dev electron .",
    "build": "vue-cli-service build --no-clean && cross-env NODE_ENV=prod electron-builder",
    "build-win": "cross-env NODE_ENV=prod electron-builder --win --x64",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "glob": "^7.1.3",
    "image-size": "^0.6.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-standard": "^3.0.4",
    "axios": "^0.18.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^5.2.0",
    "electron": "^3.0.1",
    "electron-builder": "^20.28.4",
    "element-ui": "^2.4.7",
    "express": "^4.16.3",
    "lodash": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.5",
    "moment": "^2.14.1",
    "normalize.css": "^7.0.0",
    "vue": "^2.5.17",
    "vue-axios": "^2.0.2",
    "vue-lazyload": "^1.1.4",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.20.2"
  },
  "build": {
    "appId": "com.electron.searchpicture",
    "directories": {
      "output": "e_dist"
    },
    "files": [
      "app/**/*",
      "dist/**/*",
      "config.json"
    ],
    "mac": {
      "target": "dmg",
      "icon": "public/el_icon.png"
    },
    "win": {
      "target": "zip",
      "icon": "public/el_icon.png"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
